{
  "name": "League Now Playing",
  "description": "Allows one to see information on summoners and their games/groups",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "storage"
    ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
}